<script type="text/javascript">
  function open_weibo_auth (url) {
    show_win({
      title:'授权IT桔子访问您的新浪微博帐号',
      body:'<iframe src="'+url+'" frameborder="0" width="600" height="350" onload=""></iframe>'
    });
  }

  function auth_done (msg) {
    if(msg){
      show_win({
        title:'授权IT桔子访问您的新浪微博帐号',
        body:msg
      });
    }else{
      $('#dialog').find('.exit').click();
    }
  }

  function find_friends (tar, wid, wb_url) {
    if(!is_logined()){
      open_dialog('login');
      return;
    }
    var row;
    if($('#w'+wid).length == 0){
      row = $('<tr id="w'+wid+'" style="height:0;"><td colspan="5" style="padding:10px;"></td></tr>');
    }else{
      row = $('#w'+wid);
      row.find('div').remove();
      row.animate({height:0},'fast',function(){
        row.remove();
      });
      return;
    }
    $.ajax({
      url:'/company/mm_relation?weibo_id='+wid,
      type:'GET',
      dataType:'json',
      beforeSend:function () {
        $(tar).find('.mm-loading').show();
      },
      error:function () {
        $(tar).find('.mm-loading').hide();
      },
      success:function (json) {
        $(tar).find('.mm-loading').hide();
        if(json.status=='login_required'){
          open_dialog('login');
          return;
        }else if (json.status == 'weibo_bind_required'){
          show_win({
            title:'绑定新浪微博账户', 
            body:'<p style="margin: -10px 0 20px 0;">您需要授权绑定新浪微博后才能使用此功能。</p><a class="btn btn-orange" href="javascript:void(0);" id="go_bind">立即授权绑定新浪微博</a>',
            ready:function (dialog) {
              dialog.find('#go_bind').click(function () {
                open_weibo_auth('http://itjuzi.com/oauth/sns/weibo/bind/99');
              });
            }
          });
        }else if (json.status == 'ok'){
          $(tar).parents('tr').eq(0).after(row);
          row.animate({height:50},'fast',function(){
            if(json.users.length == 0){
              row.find('td').html('<div>你们之间还没有共同好友，<a target="_blank" href="'+wb_url+'">去TA的微博看看</a>。</div>');
            }else{
              var ulist = '<p style="text-align: left;">你们的共同好友：</p>';
              ulist += '<div class="relation_list clearfix">';
              $.each(json.users,function (i, item) {
                ulist += '<a target="_blank" href="http://weibo.com/u/'+item.userID+'" title="'+item.name+'"><img src="'+item.avatar+'" title="'+item.name+'"></a>';
              });
              ulist += '</div>';
              row.find('td').html(ulist);
            }
          });
        }else{
          alert('Unknown error');
        }
      }
    });
  }
  $(function () {
    $('#com_news_more').click(function () {
      var show = $(this).attr('show') || 'less';
      if(show == 'less'){
        $(this).parent().parent().find('.hide').removeClass('hide');
        $(this).text('收起').attr('show', 'more');
      }else{
        $(this).parent().parent().find('.company-new-list :gt(2)').addClass('hide');
        $(this).text('展开更多...').attr('show', 'less');
      }
      return false;
    });

    $('#comment_content').click(function () {
      $(this).animate({height:100});
    }).blur(function () {
      if($(this).val() == ''){
        $(this).animate({height:50});
      }
    });
  });
</script>


           
